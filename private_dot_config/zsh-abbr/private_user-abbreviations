abbr -g "git a"="git add"
abbr -g "git b"="git branch"
abbr -g "git c"="git commit"
abbr -g "git co"="git checkout"
abbr -g "git d"="git diff"
abbr -g "git f"="git fetch"
abbr -g "git g"="git grep"
abbr -g "git graph"="git log -15 --branches --remotes --tags --graph --oneline --decorate=full HEAD"
abbr -g "git l"="git log"
abbr -g "git last"="git log -1 HEAD"
abbr -g "git m"="git merge"
abbr -g "git p"="git push"
abbr -g "git pr"="git pull --rebase"
abbr -g "git r"="git remote"
abbr -g "git rb"="git rebase"
abbr -g "git rbc"="git rebase --continue"
abbr -g "git rbs"="git rebase --skip"
abbr -g "git stt"="git status"
abbr -g "git unadd"="git reset HEAD"
abbr -g "git uncommit"="git reset --soft HEAD~1"
abbr -g "git uncommit-hard"="git reset --hard HEAD~1"
abbr -g "git unstage"="git reset HEAD"
abbr -g "git w"="git whatchanged"
